{
    "name": "ngx-smart-popover-demo",
    "version": "1.5.0",
    "resolutions": {
        "normalize-url": "6.1.0",
        "serialize-javascript": "6.0.0",
        "trim-newlines": "3.0.1"
    },
    "scripts": {
        "preinstall": "npx npm-force-resolutions",
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:all": "ng build && npm run build:lib",
        "build:lib": "ng build ngx-smart-popover && npm run build:lib:scss && npm run build:lib:css",
        "build:lib:css": "sass ./projects/ngx-smart-popover/src/lib/popover-content.component.scss ./dist/ngx-smart-popover/css/popover-content.component.css",
        "build:lib:scss": "mkdir ./dist/ngx-smart-popover/scss/ && cp ./projects/ngx-smart-popover/src/lib/popover-content.component.scss ./dist/ngx-smart-popover/scss/popover-content.component.scss",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "e2e:entrypoint": "npm install && ng e2e",
        "e2e:docker": "docker build -t ngx-smart-popover-e2e-tests . && docker run -v ~/code/ngx-smart-popover:/usr/src/app --rm ngx-smart-popover-e2e-tests",
        "run": "ng-packagr -p package.json",
        "publish": "cp README.md dist/ngx-smart-popover/README.md && cd dist/ngx-smart-popover && npm publish"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "8.2.14",
        "@angular/common": "8.2.14",
        "@angular/compiler": "8.2.14",
        "@angular/core": "8.2.14",
        "@angular/forms": "8.2.14",
        "@angular/platform-browser": "8.2.14",
        "@angular/platform-browser-dynamic": "8.2.14",
        "@angular/router": "8.2.14",
        "bootstrap": "4.3.1",
        "core-js": "2.6.4",
        "rxjs": "6.5.4",
        "zone.js": "0.8.29"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.803.25",
        "@angular-devkit/build-ng-packagr": "0.803.25",
        "@angular/cli": "8.3.25",
        "@angular/compiler-cli": "8.2.14",
        "@angular/language-service": "8.2.14",
        "@types/jasmine": "2.8.16",
        "@types/jasminewd2": "2.0.6",
        "@types/node": "8.9.5",
        "codelyzer": "4.5.0",
        "jasmine-core": "2.99.1",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "6.3.16",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "2.0.4",
        "karma-jasmine": "1.1.2",
        "karma-jasmine-html-reporter": "0.2.2",
        "ng-packagr": "5.1.0",
        "normalize-url": "6.1.0",
        "protractor": "5.4.2",
        "protractor-jasmine2-screenshot-reporter": "0.5.0",
        "protractor-screenshot-extension": "1.0.6",
        "sass": "^1.51.0",
        "scss-bundle": "3.1.2",
        "serialize-javascript": "6.0.0",
        "trim-newlines": "3.0.1",
        "ts-node": "7.0.1",
        "tsickle": "0.38.1",
        "tslib": "1.9.3",
        "tslint": "5.11.0",
        "typescript": "3.4.5"
    }
}
